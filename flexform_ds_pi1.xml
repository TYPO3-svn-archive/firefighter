<T3DataStructure>
 <meta>
       <langDisable>1</langDisable>
 </meta>
 <sheets>
  <sDEF>
   <ROOT>
    <TCEforms>
     <sheetTitle>LLL:EXT:firefighter/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
    </TCEforms>
    <type>array</type>
     <el>
      <detailview>
       <TCEforms>
        <exclude>1</exclude>
        <label>LLL:EXT:firefighter/locallang_db.xml:tt_content.pi_flexform.detailview</label>
        <config>
         <type>group</type>
         <internal_type>db</internal_type>
         <allowed>pages</allowed>
         <size>1</size>
         <maxitems>1</maxitems>
         <minitems>0</minitems>
         <show_thumbs>1</show_thumbs>
        </config>
       </TCEforms>
      </detailview>
      <template_file>
       <TCEforms>
        <label>LLL:EXT:firefighter/locallang_db.xml:tt_content.pi_flexform.template_file</label>
        <config>
         <type>input</type>
         <eval>trim,tx_ttnews_templateeval</eval>
         <default>EXT:firefighter/pi1/template.html</default>
         <wizards type="array">
          <_PADDING>2</_PADDING>
          <link type="array">
          <type>popup</type>
           <title>Link</title>
           <icon>link_popup.gif</icon>
           <script>browse_links.php?mode=wizard&amp;act=file</script>
           <params type="array">
            <blindLinkOptions>page,url,mail,spec</blindLinkOptions>
            <allowedExtensions>htm,html,tmpl,tpl</allowedExtensions>
           </params>
           <JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
          </link>
         </wizards>
        </config>
       </TCEforms>
      </template_file>
      <max_anzahl>
       <TCEforms>
        <label>LLL:EXT:firefighter/locallang_db.xml:tt_content.pi_flexform.max_anzahl</label>
        <config>
         <default>0</default>
         <type>input</type>
         <size>30</size>
        </config>
       </TCEforms>
      </max_anzahl>       
      <date_from>
       <TCEforms>
        <label>LLL:EXT:firefighter/locallang_db.xml:tt_content.pi_flexform.date_from</label>
        <config>
         <type>input</type>
         <eval>date</eval>
         <size>8</size>
         <max>16</max>
         <checkbox>1</checkbox>
        </config>
       </TCEforms>
      </date_from>       
      <date_to>
       <TCEforms>
        <label>LLL:EXT:firefighter/locallang_db.xml:tt_content.pi_flexform.date_to</label>
        <config>
         <type>input</type>
         <eval>date</eval>
         <size>8</size>
         <max>16</max>
         <checkbox>1</checkbox>
        </config>
       </TCEforms>
      </date_to>       
     </el>
    </ROOT>
   </sDEF>
   <display>
    <ROOT>
     <TCEforms>
      <sheetTitle>LLL:EXT:firefighter/locallang_db.xml:tt_content.pi_flexform.sheet_display</sheetTitle>
     </TCEforms>
     <type>array</type>
     <el>
      <show_search>
       <TCEforms>
        <label>LLL:EXT:firefighter/locallang_db.xml:tt_content.pi_flexform.show_search</label>
        <config>
         <type>check</type>
        </config>
       </TCEforms>
      </show_search>
     </el>
    </ROOT>
   </display>
  </sheets>
</T3DataStructure>